  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alvarosilveiraa/pure/issues"
  },
  "homepage": "https://github.com/alvarosilveiraa/pure#readme",
  "devDependencies": {
    "node-sass": "^4.5.3"
  }
}
